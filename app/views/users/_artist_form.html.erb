<%= form_for @user, :html => { :class => "artistForm email-form"} do |f| %>
	<h2>User Type 2?</h2>
	<p>Are you User Type 2? Here's some text telling you whether you should enter your email address below!</p>
	<%= render 'shared/error_messages', object: f.object %>
	<%= f.text_field :email, :placeholder => "Enter your email" %>
	<%= f.hidden_field :usertype, :value => "2" %>
	<%= f.submit "Submit", class: "btn" %>
<% end %>
